function [cost, g] = constFunc(p, ref_s)
num = length(p) / 3;
cost = 0;
g = zeros(length(p), 1);
kappa = p(num*2+1 : num*3);
g_kappa = zeros(num, 1);

for i=1:num    
    cost = cost + kappa(i)^2;
    g_kappa(i) = 2 * kappa(i);
    if i > 1
        
        cost = cost + (kappa(i) - kappa(i-1)) / 
    end
end
g(num*2+1 : num*3) = g_kappa;
